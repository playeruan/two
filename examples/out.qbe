data $glb_1 = { b "uan", b 0 }
data $glb_2 = { b "Hello, %s!\n", b 0 }
export function w $main() {
@start
	%t1 =l alloc8 16
	%t2 =l add %t1, 0
	storel $glb_1, %t2
	%t3 =l add %t1, 8
	storel 3, %t3
	%t5 =l add %t1, 0
	%t4 =l loadl %t5
	call $printf(l $glb_2, l %t4)
	%t7 =l add %t1, 8
	%t8 =l loadl %t7
	ret %t8
}
